{
  "name": "ug-gym-backend",
  "version": "1.0.0",
  "description": "University of Ghana Gym Management System Backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "migrate": "node dist/migrations/migrate.js",
    "setup-db": "bash setup-database.sh",
    "setup-db-win": "setup-database.bat",
    "setup-db-docker": "bash setup-database-docker.sh",
    "setup-db-docker-win": "setup-database-docker.bat",
    "docker:up": "docker-compose up -d postgres",
    "docker:down": "docker-compose down",
    "docker:logs": "docker logs ug_gym_postgres",
    "docker:psql": "docker exec -it ug_gym_postgres psql -U postgres -d ug_gym_db",
    "test": "jest"
  },
  "keywords": [
    "gym",
    "university",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "jest": "^29.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
